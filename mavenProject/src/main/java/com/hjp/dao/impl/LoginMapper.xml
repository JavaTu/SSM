<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hjp.dao.LoginMapper">
	
	<select id="checkUserInfo" parameterType="com.hjp.entity.UserEntity" resultType="java.lang.Integer">
		select count(*) from user where username = #{param.userName} and password = #{param.password}
	</select>
	
</mapper>

